{
    "Logging": {
      "LogLevel": {
        "Default": "Debug",
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "Serilog": {
      "Using": [ "Serilog.Sinks.Console" ],
      "MinimumLevel": "Debug",
      "Properties": {
        "Application": "Camunda-External-Task-Dispatcher"
      },
      "WriteTo": [
        { "Name": "Console" }
      ]
    },
    "Dispatcher": {
      "CamundaUrl": "http://localhost:8080/engine-rest",
      "APIMUrl": "https://{APIM name}.azure-api.net/externaltasks",
      "APIMKey": "{APIM access key}"
    }    
  }